{
  "presets": [
    "@babel/react",
    "@babel/stage-3",
    "@babel/preset-typescript",
    ["@babel/env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "babel-plugin-rewire",
    "inline-react-svg",
    "@babel/plugin-transform-runtime",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["module-resolver", {
      "extensions": [".js", ".jsx", ".ts", ".tsx"],
      "root": ["./src"]
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["styled-components", {
          "ssr": true
        }]
      ]
    }
  }
}
