{
  "presets": [
    "@babel/react",
    "@babel/stage-3",
    "@babel/preset-typescript",
    ["@babel/env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["module-resolver", {
      "extensions": [".js", ".jsx", ".ts", ".tsx"],
      "root": ["./src"]
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["styled-components", {
          "ssr": true
        }]
      ]
    }
  }
}
